# Supabase Configuration
VITE_SUPABASE_URL=your-supabase-url-here
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here

# Stripe Price IDs for Subscription Plans (Singapore Dollar)
STRIPE_PATIENCE_PRICE_ID=price_test_patience_18_sgd
STRIPE_RELIANCE_PRICE_ID=price_test_reliance_23_sgd

# Production Stripe Price IDs (replace when going live)
STRIPE_PATIENCE_PRICE_ID_PROD=price_1234567890
STRIPE_RELIANCE_PRICE_ID_PROD=price_0987654321

# Application Settings
NODE_ENV=development
VITE_APP_NAME=FaddlMatch
VITE_APP_URL=https://your-domain.com

# Email Service (if using external service)
EMAIL_SERVICE_API_KEY=your-email-service-key
EMAIL_FROM_ADDRESS=noreply@faddlmatch.com

# OpenAI API Key (for AI features)
OPENAI_API_KEY=your-openai-api-key-here